{
  "version": 2,
  "name": "fact2pdf",
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.9.0"
    },
    "api/v1/*.php": {
      "runtime": "vercel-php@0.9.0"
    }
  },
  "routes": [
    { "src": "/api/v1/(.*)", "dest": "/api/v1/$1" },
    { "src": "/assets/(.*)", "dest": "/public/assets/$1" },
    { "src": "/(.*)", "dest": "/api/index.php" }
  ],
  "env": {
    "APP_ENV": "production"
  }
}
